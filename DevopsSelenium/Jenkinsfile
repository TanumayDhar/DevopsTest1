 pipeline {
    agent any

 
 	stages {
 	
        stage('SCM Checkout') {
            steps {
                git "checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'GitCredentials', url: 'https://github.com/TanumayDhar/DevopsTest1']]])"
            }
        }
        stage('Build') {
            steps {
             
                
       			echo 'Building the application...'
                
                
            }
        }
        stage('Integration Test') {
            steps {
                echo 'Deploying in server...'
            }
        }
    }
}
 